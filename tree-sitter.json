{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "asciimath",
      "camelcase": "AsciiMath",
      "title": "AsciiMath",
      "scope": "source.asciimath",
      "file-types": [
        "asciimath"
      ],
      "injection-regex": "^asciimath$",
      "class-name": "TreeSitterAsciimath"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Asciimath grammar for tree-sitter",
    "authors": [
      {
        "name": "widcardw",
        "email": "widcardw@foxmail.com",
        "url": "https://widcard.win/"
      }
    ],
    "links": {
      "repository": "https://github.com/widcardw/tree-sitter-asciimath"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}